{
  "version": 2,
  "routes": [
    { "handle": "filesystem" },
    { "src": "/(.*)", "status": 404, "dest": "/other-404.html"},
    { "src": "/", "dest": "/index.html" },
    { "src": "/about", "dest": "/about/index.html"},
    { "src": "/academia", "dest": "/academia/index.html"},
    { "src": "/misc", "dest": "/misc/index.html"},
    { "src": "/projects", "dest": "/projects/index.html"},
    {
      "src": "/posts/.*",
      "headers": { "cache-control": "max-age=3600" },
      "continue": true
    },
    {
      "src": "/posts/([^/]+)",
      "dest": "/posts/$1"
    },
    {
      "src": "/static/images/(.*)",
      "headers": { "cache-control": "public, max-age=31536000" },
      "dest": "/static/images/$1"
    }
  ]
}
